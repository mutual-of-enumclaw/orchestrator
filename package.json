{
    "name": "@moe-tech/orchestrator",
    "author": "Mutual of Enumclaw",
    "publisher": "Mutual of Enumclaw",
    "license": "LGPLv3",
    "version": "1.0.86",
    "description": "The core data and related processing for orchestrators",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "npm run lt",
        "build": "npx tsc",
        "test": "npx jest",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "node_modules/.bin/jest --watch",
        "lint": "npx tslint -c tslint.json -p tsconfig.json",
        "lint:fix": "npx tslint -c tslint.json -p tsconfig.json --fix",
        "lt": "npm run lint && npm run test:coverage",
        "refreshVSToken": "vsts-npm-auth -config .npmrc",
        "package": "npx sls package",
        "deploy": "npx sls deploy",
        "buildPublish": "npm run build && npm publish"
    },
    "dependencies": {
        "@moe-tech/dynamo-delta": "^1.0.25",
        "@types/aws-sdk": "^2.7.0",
        "@types/rewire": "^2.5.28",
        "epsagon": "^1.58.6"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.11",
        "@types/jest": "^23.3.9",
        "@types/node": "^10.17.19",
        "aws-lambda": "^1.0.5",
        "aws-sdk": "^2.656.0",
        "aws-sdk-mock": "^4.5.0",
        "hard-source-webpack-plugin": "^0.13.1",
        "jest": "^25.3.0",
        "jest-junit": "^7.0.0",
        "serverless": "^1.75.0",
        "serverless-iam-roles-per-function": "^1.0.4",
        "serverless-plugin-generic-stack": "^1.0.2",
        "serverless-plugin-include-dependencies": "^3.2.1",
        "serverless-plugin-tracing": "^2.0.0",
        "serverless-pseudo-parameters": "^2.5.0",
        "serverless-webpack": "^5.3.1",
        "source-map-support": "^0.5.13",
        "ts-jest": "^25.3.1",
        "ts-loader": "^6.2.2",
        "tslint": "^5.18.0",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-node-externals": "^1.7.2"
    },
    "files": [
        "/dist",
        "/activity-workflow.json"
    ]
}
