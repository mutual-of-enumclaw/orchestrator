{
    "name": "@moe-tech/orchestrator",
    "author": "Mutual of Enumclaw",
    "publisher": "Mutual of Enumclaw",
    "license": "LGPLv3",
    "version": "1.0.97",
    "description": "The core data and related processing for orchestrators",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run buildLibrary && npm run buildActivity && npm run buildMetrics && npm run buildCore",
        "test": "npx jest",
        "installAll": "npm run installHS && npm run installIA && npm run installActivity && npm run installLibrary && npm run installMetrics",
        "installHS": "cd src/functions/handleSummary && npm i",
        "installIA": "cd src/functions/initializeActivities && npm i",
        "installActivity": "cd activity && npm i",
        "installLibrary": "cd library && npm i",
        "installMetrics": "cd metrics && npm i",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "node_modules/.bin/jest --watch",
        "lint": "npx eslint **/*.ts --ignore-pattern **/*.d.ts",
        "lint:fix": "npm run lint --fix",
        "refreshVSToken": "vsts-npm-auth -config .npmrc",
        "activities": "npx orch-utils activities",
        "clean": "npx orch-utils clean",
        "buildLibrary": "cd library && npm run build",
        "buildActivity": "cd activity && npm i && npm run build",
        "buildMetrics": "cd metrics && npm i && npm run build",
        "buildCore": "npm i && npx tsc -d",
        "deploy": "sam deploy --capabilities CAPABILITY_NAMED_IAM CAPABILITY_AUTO_EXPAND",
        "buildPublish": "npm run build && npm publish",
        "deployNk": "npm run deploy -- --stack-name nk-orch --s3-bucket moe-deployment-snd --s3-prefix sam/nk-orch --parameter-overrides StackTagName=nk-orch EnvironmentTagName=snd"
    },
    "dependencies": {},
    "devDependencies": {
        "@moe-tech/orch-utils": "file:orch-utils",
        "@types/jest": "^23.3.9",
        "@types/node": "^10.17.19",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "eslint": "^7.14.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-config-standard-with-typescript": "^19.0.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.1.0",
        "jest": "^25.5.4",
        "jest-junit": "^7.0.0",
        "source-map-support": "^0.5.13",
        "ts-jest": "^25.3.1",
        "ts-loader": "^6.2.2",
        "typescript": "^3.8.3"
    },
    "files": [
        "/dist",
        "/activity-workflow.json"
    ]
}
