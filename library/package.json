    "version": "1.0.97",
    "description": "The core data and related processing for orchestrators",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "npm run lt",
        "build": "npx tsc",
        "test": "npx jest",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "node_modules/.bin/jest --watch",
        "lint": "npx tslint -c tslint.json -p tsconfig.json",
        "lint:fix": "npx tslint -c tslint.json -p tsconfig.json --fix",
        "lt": "npm run lint && npm run test:coverage",
        "buildPublish": "npm run build && npm publish"
    },
